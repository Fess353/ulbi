  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "prod": "webpack --env mode=production",
    "dev": "webpack --env mode=development",
    "eslint": "npx eslint \"**/*.{ts,tsx}\"",
    "eslint:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:init": "npx ts-jest config:init",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki update --requireReference --reactUri file:./storybook-static",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "visual": "npm run visual:json && npm run visual:html",
    "visual:json": "npx node scripts/generate-visual-json-report.js",
    "visual:html": "npx reg-cli -g --from .loki/report.json --report .loki/report.html"
  },
